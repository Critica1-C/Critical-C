{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/prop-types": "off",
    "import/no-named-as-default": 0
  },
  "env": {
    "browser": true
  }
}
